@PACKAGE_INIT@

INCLUDE( CMakeFindDependencyMacro )
FIND_DEPENDENCY( Threads )
SET(CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
FIND_PACKAGE( Boost 1.55 REQUIRED COMPONENTS log )

INCLUDE( ${CMAKE_CURRENT_LIST_DIR}/flexcore-targets.cmake )

SET_AND_CHECK( flexcore_INCLUDE_DIR "@PACKAGE_INSTALL_INCLUDE_DIR@" )
SET_AND_CHECK( flexcore_LIBRARY_DIR "@PACKAGE_INSTALL_LIB_DIR@" )
SET( flexcore_LIBRARY flexcore )
GET_PROPERTY( flexcore_COMPILE_OPTIONS TARGET flexcore PROPERTY INTERFACE_COMPILE_OPTIONS )

